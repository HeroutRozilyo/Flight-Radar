<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PFlight.views"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes" x:Class="PFlight.views.firstWindow"
        mc:Ignorable="d"
        Title="firstWindow" Height="450" Width="800">

    <Grid Margin="0,0,0.333,-0.333">
        <Grid.Background>
            <ImageBrush ImageSource="/Icon/open.jpg" Opacity="1" />

        </Grid.Background>

        <Button x:Name="seeF" Content="StartTrack" Command="{Binding OpenCommand}" HorizontalAlignment="Left" Margin="290,273,0,0" VerticalAlignment="Top" Width="159" Height="50"/>
       

       



    </Grid>
</Window>
